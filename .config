{
  "dataset_dir": "SWE-bench_Lite",
  "dataset_files": [
    "data/test-00000-of-00001.parquet"
  ],
  "repos_dir": "repos",
  "indexes_dir": "indexes",
  "outputs_dir": "outputs",
  "logs_dir": "logs",
  "bins_dir": "bin",
  "bm25": { "k1": 1.5, "b": 0.75 },
  "retrieval": { "top_k": 10 },
  "indexing": {
    "max_file_size_bytes": 1048576,
    "include_extensions": [".py"],
    "exclude_dirs": [".git", "venv", "env", "__pycache__", "build", "dist", "node_modules", ".tox", ".mypy_cache"]
  },
  "clone": { "enable": true, "base_url": "https://github.com" },
  "concurrency": { "index_workers": 4 }
}
